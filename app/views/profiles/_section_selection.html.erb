<div class="col-md-2">
</div>
<div class="col-md-10">
	<%if @pro_id %>
	<%@sections.each do |sec|%>
	<div class="panel panel-primary">
		<div class="panel-heading">
			<h3 id="panel-title" class="panel-title"> <%= sec.name %> </h3>
		</div>
		<div class="panel-body">
			<% @questions_arr = sec.questions.all%>
			<%if @questions_arr %>
			<ul class="list-group">
				<%@questions_arr.each do |que|%>
				<li class="list-group-item">
					<%= que.question %>
					<input type="hidden" value="<%= que.id %>" />
					<%if que.question_type == "Textbox" %>
					<input type="text" />
					<% elsif que.question_type == "Dropdown" %>
					<% dpv = que.question_value.split(',')%>
					<%= select_tag :question_value, options_for_select(dpv) %>
					<% elsif que.question_type == "Radiobutton" %>
					<% dpv = que.question_value.split(',')%>
					<%dpv.each do |rv|%>
					<input type="radio" id="rb"  value="" class="checkbox-inline"/><%= rv %>
					 <%end%>
					 <% elsif que.question_type == "Checkbox" %>
					 <%if que.question_value%>
					<% dpv = que.question_value.split(',')%>
					<%dpv.each do |rv|%>
					<input type="checkbox" id="rb"  value="" class="checkbox-inline"/><%= rv %>
					 <%end%>
					 <%end%>
					<%end%>
				</li>
				<%end%>
			</ul>
			<%end%>
		</div>
	</div>
	<%end%>
	<%end%>
</div>
