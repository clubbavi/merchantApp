<h1>Listing profile_answeres</h1>

<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th>Question</th>
      <th>Answere</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profile_answeres.each do |profile_answere| %>
      <tr>
        <td><%= profile_answere.profile_id %></td>
        <td><%= profile_answere.question_id %></td>
        <td><%= profile_answere.answere %></td>
        <td><%= link_to 'Show', profile_answere %></td>
        <td><%= link_to 'Edit', edit_profile_answere_path(profile_answere) %></td>
        <td><%= link_to 'Destroy', profile_answere, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile answere', new_profile_answere_path %>
